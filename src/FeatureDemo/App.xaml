<Application
    x:Class="FeatureDemo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.WinUI.Core"
    xmlns:Common="using:FeatureDemo.Common"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <Common:CommonStyles />
                <ResourceDictionary Source="RibbonDemo/RibbonDemoCommonStyles.xaml"/>
                <ResourceDictionary Source="Common/CodeViewControl/Themes/CodeViewControl.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <x:String x:Key="AppName">FeatureDemo</x:String>
        </ResourceDictionary>
    </Application.Resources>

</Application>